{
  "name": "move-it",
  "version": "0.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo --port 4201",
    "build:demo": "ng build demo --prod",
    "build:js": "ng build --prod move-it",
    "copy-css": "cp ./projects/move-it/src/lib/move-it.css ./dist/move-it/lib",
    "build:lib": "npm run build:js && npm run copy-css",
    "copy-readme": "cp ./README.md ./dist/move-it",
    "copy-license": "cp ./LICENSE ./dist/move-it",
    "copy-files": "npm run copy-readme && npm run copy-license",
    "npm_pack": "cd dist/move-it && npm pack",
    "package": "npm run build:lib && npm run copy-files && npm run npm_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "core-js": "^2.5.4",
    "ng-packagr": "^4.4.5",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~7.3.10",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "^3.2.4"
  }
}