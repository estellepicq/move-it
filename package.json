{
  "name": "move-it",
  "version": "0.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo --port 4201",
    "build:demo": "ng build demo --prod",
    "build:js": "ng build --prod move-it",
    "copy-css": "cp ./projects/move-it/src/lib/move-it.css ./dist/move-it/lib",
    "build:lib": "npm run build:js && npm run copy-css",
    "copy-readme": "cp ./README.md ./dist/move-it",
    "copy-license": "cp ./LICENSE ./dist/move-it",
    "copy-files": "npm run copy-readme && npm run copy-license",
    "npm_pack": "cd dist/move-it && npm pack",
    "package": "npm run build:lib && npm run copy-files && npm run npm_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^13.3.1",
    "@angular/common": "^13.3.1",
    "@angular/compiler": "^13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/forms": "^13.3.1",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/router": "^13.3.1",
    "core-js": "^3.21.1",
    "rxjs": "^7.5.5",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "^13.3.1",
    "@angular/language-service": "^13.3.1",
    "@types/jasmine": "^4.0.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "~17.0.23",
    "codelyzer": "~6.0.2",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.7.0",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "typescript": "^4.6.3"
  }
}
